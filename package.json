{
  "name": "@synterra/shared",
  "version": "8.0.2",
  "description": "Nest - empty template to create lib",
  "author": "Hugo Blanc",
  "license": "MIT",
  "url": "https://github.com/nestjs/typeorm#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand",
    "test:e2e:dev": "jest --config ./tests/jest-e2e.json --runInBand --watch",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "devDependencies": {
    "@nestjs/common": "8.0.6",
    "@nestjs/core": "8.0.6",
    "@nestjs/platform-express": "8.0.6",
    "@synterra/spinal-rxjs": "^0.1.0",
    "@types/node": "16.0.0",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "jest": "27.1.0",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0",
    "spinal-core-connectorjs": "git+https://github.com/spinalcom/spinal-core-connectorjs.git",
    "spinal-core-connectorjs_type": "git+https://github.com/spinalcom/spinal-core-connectorjs_type.git",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjs/typeorm"
  }
}
